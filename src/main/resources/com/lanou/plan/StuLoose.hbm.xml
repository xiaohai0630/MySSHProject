<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <!--学生流失情况信息表-->
    <class name="com.lanou.plan_dep.domain.StuLoose">
        <id name="runOffId" length="32">
            <generator class="native"/>
        </id>
        <property name="staffId" type="java.lang.String"></property>
        <property name="createDate" type="java.util.Date"></property>
        <property name="isRefund" type="java.lang.String"></property>
        <property name="refundAmount" type="java.lang.Double"></property>
        <property name="remark" type="java.lang.String"></property>

    </class>

</hibernate-mapping>
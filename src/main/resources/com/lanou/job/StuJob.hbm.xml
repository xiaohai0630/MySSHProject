<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <!--学生就业信息表-->
    <class name="com.lanou.job_dep.domain.StuJob">
        <id name="granduateId" length="32">
            <generator class="native"/>
        </id>
        <property name="companyName" type="java.lang.String"></property>
        <property name="salary" type="java.lang.Double"></property>
        <property name="post" type="java.lang.String"></property>
        <property name="entryTime" type="java.util.Date"></property>
        <property name="remark" type="java.lang.String"></property>

    </class>

</hibernate-mapping>
